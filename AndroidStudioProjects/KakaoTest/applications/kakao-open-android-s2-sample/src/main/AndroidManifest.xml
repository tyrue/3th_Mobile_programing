<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kakao.sdk.s2.sample">

    <application
        android:name=".S2Application"
        android:allowBackup="false"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:icon="@drawable/kakaolink_sample_icon"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key"/>

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".S2MainActivity">
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/text_settings">
            >
        </activity>
        <activity
            android:name=".FirstActivity"
            android:label="@string/text_activity1">
            >
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="@string/text_activity2">
            >
        </activity>
        <activity
            android:name=".ThirdActivity"
            android:label="@string/text_activity3">
            >
        </activity>
        <activity android:name=".S2BaseActivity">
        </activity>
    </application>

</manifest>